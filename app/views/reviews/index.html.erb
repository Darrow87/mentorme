
<div id="reviews">
	<h1>Reviews</h1>
	<div id="received">
		Reviews you have received:<br>
		<% @feedbacks.each do |feedback| %>
			<div class="rev-blurb">
				Rating: <%= feedback.rating %><br>
				<%= feedback.author.full_name %>: "<%= feedback.body %>"<br>
				<br>
			</div>
		<% end %>
	</div>

	<div id="written">
		Reviews you have written:<br>
		<%= render @reviews %><br>
	</div>

</div>